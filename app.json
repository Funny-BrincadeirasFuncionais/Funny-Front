{
  "expo": {
    "name": "funny-frontend",
    "slug": "funny-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "funnyfrontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon_funny.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "icon": "./assets/images/icon_funny.png",

    "splash": {
      "image": "./assets/images/funny_original.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ca5a55bc-dd64-4438-9907-5a383aaca213"
      }
    },
    "owner": "yasslopes-org"
  }
}